{
  "name": "portfolio-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portfolio-frontend",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/portfolio-frontend",
        "outputPath": "dist/apps/portfolio-frontend"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/portfolio-frontend"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "portfolio-frontend:build",
        "dev": true,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "portfolio-frontend:build:development",
          "dev": true,
          "port": 4200
        },
        "production": {
          "buildTarget": "portfolio-frontend:build:production",
          "dev": false,
          "port": 4200
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "portfolio-frontend:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}